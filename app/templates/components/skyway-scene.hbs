<section class="scene-screen">
  <section class="scene">
    <img class="skyway-entrance-scene" src="images/skywayEntranceBg.png">
    <div id="car" class="exit-skyway thing" data-name="Exit" onclick={{action thingClicked}} onmouseover={{action helper}} onmouseout={{action clearHelper}}></div>
    <section class="walkable-area skyway-entrance-scene" {{action walk}}></section>

    <div class="rain-container-skyway">
      <img class="rain" src="/sprites/rain.png">
    </div>

    <div id="pedestrian1" class="pedestrians">
      <img class="pedestrianSprite" src="/sprites/Pedestrian.png">
    </div>
    <div id="pedestrian2" class="pedestrians">
      <img class="pedestrianSprite" src="/sprites/Pedestrian2.png">
    </div>

  </section>
</section>